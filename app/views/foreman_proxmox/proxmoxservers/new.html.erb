<%= form_for @proxmox do |f| %>
    <%= f.label :ip %>
    <%= f.text_field :ip %>
    <br>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <br>
    <%= f.label :storage %>
    <%= f.text_field :storage %>
    <br>
    <%= f.label :bridge %>
    <%= f.text_field :bridge %>
    <br>
    <%= f.submit 'Create' %>
<% end %>
